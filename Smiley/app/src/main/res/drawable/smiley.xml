<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/faceLayer">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">
            <solid android:color="@color/clear" />
            <size android:height="50dp" android:width="50dp"/>

        </shape>
    </item>
    <item android:top="0dp" android:left="5dp" android:right="35dp" android:bottom="35dp">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">
            <solid android:color="@color/black"/>
            <size android:height="2dp" android:width="2dp"/>
        </shape>
    </item>
    <item android:top="0dp" android:left="35dp" android:right="5dp" android:bottom="35dp">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">
            <solid android:color="@color/black"/>
            <size android:height="2dp" android:width="2dp"/>
        </shape>
    </item>
</layer-list>